{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "strict": true,
    "allowJs": true,
    "checkJs": true,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "rootDir": ".",
    "outDir": "build",
    "types": [
      "node"
    ],
    "lib": [
      "es2015",
      "dom"
    ]
  },
  "include": [
    "src/**/*",
    "test/**/*"
  ]
}
